  #include <stdio.h >
  int main () { 
  int m[10] [10]
  int i ,j , x;
     for(i=0;i<10;i++)
          for(j=0;j<10;j++)
              if(i==j)
                printf('m[i][j]=\n'); 
          for(j=0;j<10;j++)
             for (i=0;i<10;i++)
            {  
               m[i][j]=m[j][i]; 
             printf("%c",matrix[i][j]);
             printf("\n");
             return 0;
               }
